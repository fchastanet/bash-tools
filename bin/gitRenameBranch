  assumeYesHelpFunction



  echo -e "  ${__HELP_OPTION_COLOR}--push${__HELP_NORMAL}, ${__HELP_OPTION_COLOR}-p${__HELP_NORMAL} {single}"
  Array::wrap2 ' ' 76 4 "    " "Push the new branch."
  echo


  echo -e "  ${__HELP_OPTION_COLOR}--delete${__HELP_NORMAL}, ${__HELP_OPTION_COLOR}-d${__HELP_NORMAL} {single}"
  Array::wrap2 ' ' 76 4 "    " "Delete the old remote branch."
  echo

  # ------------------------------------------
  # longDescription section
  # ------------------------------------------
  echo
  echo
  echo -e "${__HELP_TITLE_COLOR}DESCRIPTION:${__RESET_COLOR}"
  longDescriptionFunction
  # ------------------------------------------
  # version section
  # ------------------------------------------
  echo

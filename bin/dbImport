          optionFromAws="$1"
          ;;
        # Option 6/22
        # Option optionTargetDsn --target-dsn|-t variableType String min 0 max 1 authorizedValues '' regexp ''
        --target-dsn | -t)
          shift
          if (($# == 0)); then
            Log::displayError "Command ${SCRIPT_NAME} - Option ${options_parse_arg} - a value needs to be specified"
